msgid "Copy to Clipboard"
msgstr "Kopiraj u međuspremnik"

#: src/window.ui:72 src/welcome.ui:67
msgid "Keyboard Shortcuts"
msgstr "Tipkovnički prečaci"

#: src/window.ui:76 src/welcome.ui:71
msgid "About Junction"
msgstr "O programu Junction"

#: src/welcome.ui:37
msgid "Test Junction"
msgstr "Isprobaj Junction"

#: src/welcome.ui:48
msgid "Set Junction as default for Web"
msgstr "Postavi kao zadani program za web"

#: src/AppButton.js:115
msgid "View All"
msgstr "Prikaži sve"

#: src/AppButton.js:161
msgid "All Applications"
msgstr "Svi programi"

#: src/Entry.js:23
msgid "This site has no security. An attacker could see any information you send, or control the content that you see."
msgstr "Ova stranica nema sigurnost. Napadač bi mogao vidjeti bilo koju informaciju koju šalješ ili upravljati sadržajem koji vidiš."
